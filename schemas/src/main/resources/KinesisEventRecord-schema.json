{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "awsRegion" : {
      "type" : "string"
    },
    "eventID" : {
      "type" : "string"
    },
    "eventName" : {
      "type" : "string"
    },
    "eventSource" : {
      "type" : "string"
    },
    "eventSourceARN" : {
      "type" : "string"
    },
    "eventVersion" : {
      "type" : "string"
    },
    "invokeIdentityArn" : {
      "type" : "string"
    },
    "kinesis" : {
      "type" : "object",
      "properties" : {
        "approximateArrivalTimestamp" : {
          "type" : "string"
        },
        "data" : {
          "type" : "object",
          "properties" : {
            "address" : {
              "type" : "integer"
            },
            "bigEndian" : {
              "type" : "boolean"
            },
            "capacity" : {
              "type" : "integer"
            },
            "hb" : {
              "type" : "array",
              "items" : {
                "type" : "string"
              }
            },
            "isReadOnly" : {
              "type" : "boolean"
            },
            "limit" : {
              "type" : "integer"
            },
            "mark" : {
              "type" : "integer"
            },
            "nativeByteOrder" : {
              "type" : "boolean"
            },
            "offset" : {
              "type" : "integer"
            },
            "position" : {
              "type" : "integer"
            },
            "segment" : {
              "type" : "object"
            }
          }
        },
        "encryptionType" : {
          "type" : "string"
        },
        "kinesisSchemaVersion" : {
          "type" : "string"
        },
        "partitionKey" : {
          "type" : "string"
        },
        "sequenceNumber" : {
          "type" : "string"
        }
      }
    }
  }
}